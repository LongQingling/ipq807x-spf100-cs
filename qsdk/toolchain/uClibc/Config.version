config UCLIBC_VERSION
	string
	depends on USE_UCLIBC
	default "1.0.17"		if UCLIBC_VERSION_1_0_17
	default "1.0.14"		if UCLIBC_VERSION_1_0_14
	default "0.9.33.2"       if UCLIBC_VERSION_0_9_33
	default "1.0.14"

config UCLIBC_VERSION_0_9_33
	bool

config UCLIBC_VERSION_1_0_14
	default y if !TOOLCHAINOPTS && USE_UCLIBC
	bool

config UCLIBC_VERSION_1_0_17
	bool

config UCLIBC_VERSION_NG
	bool
	depends on USE_UCLIBC
	default y
